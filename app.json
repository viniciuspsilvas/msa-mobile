{
  "expo": {
    "name": "Mindroom Student APP",
    "slug": "mindroom-student-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#E54236"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#E54236"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "au.edu.mindroom.msa"
    },
    "android": {
      "package": "au.edu.mindroom.msa",
      "googleServicesFile": "./google-services.json",
      "permissions" : [],
      "versionCode": 3
    }
  },
  "name": "msa-mobile",

  "updates": {
    "enabled": false
  }
}